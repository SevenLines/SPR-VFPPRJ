Local q, k
Local miststr

q = "EXEC up_kont 1"
k = sqlexec(lcn, q, 'up_kont')
if k <= 0
 aerror(asd)
 messagebox(asd[2])
 return .F.
endif

miststr = ""
select up_kont
scan
 if isnull(id_1)
  miststr = miststr + "курс " + ltrim(str(shkurs)) + " УП " + rtrim(shifr) + "(ДФО) вкл. в РУП, но не связан с контингентом" + chr(13)
 endif
endscan
use

if !empty(miststr)
 messagebox(miststr)
 return .F.
endif
return .T.